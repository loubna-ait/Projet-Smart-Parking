<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ma.emsi.entities">
    <class name="PlaceStationnement" table="PlaceStationnement">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="numero" column="numero" not-null="true"/>
        <property name="etage" column="etage"/>
        <property name="type" column="type"/>
        <property name="estDisponible" column="est_disponible"/>

        <!-- Many-to-One mapping: A Place belongs to one Parking -->
        <many-to-one name="parking" class="Parking" column="parking_id" not-null="true"/>
    </class>
</hibernate-mapping>
